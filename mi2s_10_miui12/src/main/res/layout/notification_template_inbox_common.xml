<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:internal="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
        <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/inbox" android:layout_width="match_parent" android:layout_height="@dimen/notification_height" android:paddingStart="@dimen/notification_padding_left" android:paddingEnd="@dimen/notification_padding_right">
            <ImageView android:layout_gravity="center_vertical" android:id="@+id/icon" android:layout_width="@dimen/notification_normal_icon_width" android:layout_height="@dimen/notification_normal_icon_height" android:scaleType="centerInside"/>
            <LinearLayout android:gravity="center_vertical" android:orientation="vertical" android:id="@+id/content_container" android:paddingTop="2dp" android:paddingBottom="2dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:minHeight="@dimen/notification_normal_icon_height" android:layout_marginStart="@dimen/notification_content_margin_left_to_icon">
                <LinearLayout android:orientation="horizontal" android:id="@+id/line1" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_weight="0">
                    <TextView android:textAppearance="@style/TextAppearance.StatusBar.EventContent.Title" android:ellipsize="marquee" android:id="@+id/title" android:fadingEdge="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:singleLine="true" android:layout_weight="1"/>
                    <include layout="@layout/notification_template_part_time"/>
                    <ViewStub android:layout_gravity="center" android:id="@+id/chronometer" android:visibility="gone" android:layout="@layout/notification_template_part_chronometer" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_weight="0"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/line3" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="-2dp" android:layout_weight="0">
                    <TextView android:textAppearance="@style/TextAppearance.StatusBar.EventContent" android:ellipsize="marquee" android:layout_gravity="center" android:id="@+id/text" android:fadingEdge="horizontal" android:layout_width="0dp" android:layout_height="wrap_content" android:singleLine="true" android:layout_weight="1"/>
                    <TextView android:textAppearance="@style/TextAppearance.StatusBar.EventContent.Info" android:gravity="center" android:layout_gravity="center" android:id="@+id/info" android:layout_width="wrap_content" android:layout_height="wrap_content" android:singleLine="true" android:layout_weight="0" android:paddingStart="8dp"/>
                    <ImageView android:layout_gravity="center" android:id="@+id/profile_badge_line2" android:visibility="gone" android:layout_width="16dp" android:layout_height="16dp" android:scaleType="centerInside" android:layout_weight="0" android:layout_marginStart="8dp" android:drawableAlpha="153"/>
                    <ImageView android:layout_gravity="center" android:id="@+id/profile_badge_line3" android:visibility="gone" android:layout_width="16dp" android:layout_height="16dp" android:scaleType="centerInside" android:layout_weight="0" android:layout_marginStart="8dp" android:drawableAlpha="153"/>
                    <ImageView android:layout_gravity="center" android:id="@+id/profile_badge_large_template" android:visibility="gone" android:layout_width="16dp" android:layout_height="16dp" android:scaleType="centerInside" android:layout_weight="0" android:layout_marginStart="8dp" android:drawableAlpha="153"/>
                    <ImageView android:id="@+id/right_icon" android:visibility="gone" android:layout_width="0dp" android:layout_height="0dp" android:layout_weight="0"/>
                </LinearLayout>
                <TextView android:textAppearance="@style/TextAppearance.StatusBar.EventContent.Line2" android:ellipsize="marquee" android:id="@+id/text2" android:visibility="gone" android:fadingEdge="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="-2dp" android:layout_marginBottom="-2dp" android:singleLine="true" android:layout_weight="0"/>
                <miui.widget.ProgressBar android:id="@+id/progress" android:visibility="gone" android:layout_width="match_parent" android:layout_height="12dp" android:layout_weight="0" style="?attr/progressBarStyleHorizontal"/>
            </LinearLayout>
        </LinearLayout>
        <ImageView android:id="@+id/action_divider" android:background="@drawable/list_dashed_divider" android:visibility="gone" android:layout_width="match_parent" android:layout_height="1px"/>
        <FrameLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginStart="@dimen/notification_content_margin_left_to_icon">
            <include android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginStart="@dimen/notification_normal_icon_width" layout="@layout/notification_action_list"/>
        </FrameLayout>
        <ImageView android:id="@+id/overflow_divider" android:background="@drawable/list_dashed_divider" android:visibility="visible" android:layout_width="match_parent" android:layout_height="1px" android:layout_marginTop="2dp"/>
        <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="8dp" android:paddingStart="@dimen/notification_content_margin_left_to_icon" android:paddingEnd="@dimen/notification_padding_right" android:layout_marginStart="@dimen/notification_normal_icon_width">
            <TextView android:textAppearance="@style/TextAppearance.StatusBar.EventContent" android:ellipsize="end" android:id="@+id/inbox_text0" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:singleLine="true"/>
            <TextView android:textAppearance="@style/TextAppearance.StatusBar.EventContent" android:ellipsize="end" android:id="@+id/inbox_text1" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:singleLine="true"/>
            <TextView android:textAppearance="@style/TextAppearance.StatusBar.EventContent" android:ellipsize="end" android:id="@+id/inbox_text2" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:singleLine="true"/>
            <TextView android:textAppearance="@style/TextAppearance.StatusBar.EventContent" android:ellipsize="end" android:id="@+id/inbox_text3" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:singleLine="true"/>
            <TextView android:textAppearance="@style/TextAppearance.StatusBar.EventContent" android:ellipsize="end" android:id="@+id/inbox_text4" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:singleLine="true"/>
            <TextView android:textAppearance="@style/TextAppearance.StatusBar.EventContent" android:ellipsize="end" android:id="@+id/inbox_text5" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:singleLine="true"/>
            <TextView android:textAppearance="@style/TextAppearance.StatusBar.EventContent" android:ellipsize="end" android:id="@+id/inbox_text6" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:singleLine="true"/>
            <TextView android:textAppearance="@style/TextAppearance.StatusBar.EventContent" android:ellipsize="end" android:id="@+id/inbox_more" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/ellipsis" android:singleLine="true"/>
            <FrameLayout android:id="@+id/inbox_end_pad" android:visibility="gone" android:layout_width="match_parent" android:layout_height="8dp" android:layout_weight="0"/>
        </LinearLayout>
    </LinearLayout>
</merge>
