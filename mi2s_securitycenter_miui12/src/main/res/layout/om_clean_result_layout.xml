<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:securitycenter="http://schemas.android.com/apk/res/com.miui.securitycenter" xmlns:miui="http://schemas.android.com/apk/res/miui" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:orientation="vertical" android:id="@+id/clean_finish_layout" android:layout_width="match_parent" android:layout_height="@dimen/om_main_top_height" android:layout_marginTop="@dimen/om_fragment_content_margin_top">
        <RelativeLayout android:id="@+id/result_icon_content" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginTop="1.8dp">
            <ImageView android:id="@+id/clean_finish_icon" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/result_top_icon" android:layout_centerHorizontal="true"/>
            <ImageView android:id="@+id/clean_little_icon" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/om_result_score_icon_margin_top" android:src="@drawable/optimizemanage_result_icon" android:layout_centerHorizontal="true"/>
        </RelativeLayout>
        <TextView android:textSize="@dimen/om_result_top_finish_text_size" android:textStyle="bold" android:textColor="@color/om_result_finish_text_color" android:id="@+id/clean_finish_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/om_result_top_finish_text_margin_top" android:layout_alignParentTop="true" android:layout_centerHorizontal="true"/>
        <TextView android:textSize="@dimen/om_clean_memory_tip_text_size" android:textColor="@color/om_clean_memory_size_tip_color" android:id="@+id/clean_memory_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/om_result_memory_text_margin_top" android:layout_below="@+id/clean_finish_text" android:layout_centerHorizontal="true"/>
    </RelativeLayout>
    <com.miui.common.customview.AutoPasteListView android:orientation="vertical" android:id="@+id/clean_result" android:scrollbars="none" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginTop="@dimen/om_fragment_content_margin_top" android:paddingStart="@dimen/card_layout_auto_paste_list_view_margin_se" android:paddingEnd="@dimen/card_layout_auto_paste_list_view_margin_se"/>
</RelativeLayout>
