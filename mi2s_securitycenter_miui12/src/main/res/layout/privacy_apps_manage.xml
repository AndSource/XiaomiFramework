    <TextView android:textAppearance="0x100d009d" android:layout_gravity="center" android:id="@+id/empty_view" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/app_managere_not_have_app" android:drawableTop="@drawable/icon_list_empty"/>
