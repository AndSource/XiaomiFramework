            <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
                <com.miui.appmanager.widget.AppDetailTitleView android:id="@+id/am_detail_title" android:layout_width="match_parent" android:layout_height="wrap_content"/>
                <com.miui.appmanager.widget.AppDetailBannerItemView android:layout_gravity="center" android:id="@+id/am_storage_view" android:background="@drawable/am_card_bg_selector" android:layout_width="match_parent" android:layout_height="wrap_content"/>
