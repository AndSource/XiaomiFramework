                <include layout="@layout/app_manager_separation_layout"/>
                <com.miui.appmanager.widget.AppDetailListTitleView android:id="@+id/am_detail_perm_title" android:layout_width="match_parent" android:layout_height="wrap_content"/>
                <com.miui.appmanager.widget.AppDetailRightSummaryPointView android:id="@+id/am_detail_behavior" android:layout_width="match_parent" android:layout_height="wrap_content"/>
                <com.miui.appmanager.widget.AppDetailCheckBoxView android:id="@+id/am_detail_as" android:layout_width="match_parent" android:layout_height="wrap_content"/>
                <com.miui.appmanager.widget.AppDetailTextBannerView android:id="@+id/am_global_perm" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content"/>
                <com.miui.appmanager.widget.AppDetailBannerItemView android:id="@+id/am_detail_perm" android:layout_width="match_parent" android:layout_height="wrap_content"/>
                <com.miui.appmanager.widget.AppDetailBannerItemView android:id="@+id/am_hydrid_perm" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content"/>
                <com.miui.appmanager.widget.AppDetailBannerItemView android:id="@+id/am_detail_notify" android:layout_width="match_parent" android:layout_height="wrap_content"/>
                <com.miui.appmanager.widget.AppDetailBannerItemView android:id="@+id/am_detail_net" android:layout_width="match_parent" android:layout_height="wrap_content"/>
                <com.miui.appmanager.widget.AppDetailBannerItemView android:id="@+id/am_detail_keeper" android:layout_width="match_parent" android:layout_height="wrap_content"/>
                <include layout="@layout/app_manager_separation_layout"/>
                <com.miui.appmanager.widget.AppDetailListTitleView android:id="@+id/am_detail_default_title" android:layout_width="match_parent" android:layout_height="wrap_content"/>
                <com.miui.appmanager.widget.AppDetailCheckBoxView android:id="@+id/am_full_screen" android:layout_width="match_parent" android:layout_height="wrap_content"/>
                <com.miui.appmanager.widget.AppDetailCheckBoxView android:id="@+id/am_thumbnail_blur" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content"/>
                <com.miui.appmanager.widget.AppDetailBannerItemView android:id="@+id/am_potentiaL_app" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content"/>
                <com.miui.appmanager.widget.AppDetailTextBannerView android:id="@+id/am_detail_default" android:layout_width="match_parent" android:layout_height="wrap_content"/>
                <View android:id="@+id/am_empty_view" android:layout_width="match_parent" android:layout_height="wrap_content"/>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </miuix.springback.view.SpringBackLayout>
</LinearLayout>
