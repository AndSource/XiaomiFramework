                        <TextView android:textSize="@dimen/text_font_size_40" android:textColor="#9cb5b5" android:id="@+id/title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/doze_optimization_title" android:textDirection="locale"/>
                        <TextView android:textSize="@dimen/text_font_size_34" android:textColor="#586668" android:id="@+id/summary" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/view_dimen_5" android:text="@string/doze_optimization_summary" android:textDirection="locale"/>
